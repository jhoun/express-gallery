<h1>My Projects</h1>
<form form="form" action="/portfolio/new" method="GET" enctype="x-www-form-urlencoded">
  <input type="submit" value="Add New Project">
</form>

<ul>
{{#each project}}
  <li>Author: {{this.author}}</li>
  <li>Title: {{this.title}}</li>
  <li>Link: {{this.link}}</li>
  <li>Description: {{this.description}}</li>
  <form form="form" action="/portfolio/{{id}}" method="GET" enctype="x-www-form-urlencoded">
  <input type="submit" value="More Info">
  </form>
  <br>
{{/each}}
</ul>
